  <div style="width: 70%; " class="jumbotron m-3 mx-auto rounded-lg shadow-lg" style="background-color: #ffffff; margin-bottom: 40px;">
    <h2 class="display-4" style="text-align: center;">Submissions made for the Assignment</h2>
    <hr style="width: 1000px;margin: 0 auto;">
  <p style="text-align: center; margin-top: 15px;">You can view all the assignments submitted by the students.</p>
  </div>

  <!-- </div> -->
  

  <div>
    <table style="width: 75%; margin: 50px auto; "class="table table-striped">
      <thead>
        <tr>
          <!-- <th scope="col">#</th> -->
          <th scope="col">First Name</th>
          <th scope="col">Last Name</th>
          <th scope="col">Email Id</th>
          <th scope="col">File Submitted</th>
          <th scope="col">Assign Points</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
          <% array.forEach((element) => { %>
            <form action="/viewSubmissions/<%= element.student._id%>/<%=assignment_id%>" method="POST">
            <tr>
              <!-- <th scope="row">1</th> -->
              <td><%= element.student.firstName %></td>
              <td><%= element.student.lastName %></td>
              <td><%= element.student.email %></td>
              <td><a href="<%= element.pdfLink %>" target="_blank">Uploaded Attachment</a></td>
              <% if (!element.isGraded){ %>
                <td><input style="width: 70px;" type="number" min="0" name="marks"></td>
              <% } %>
        
              <td><button style="padding: 0px 20px;" type="submit" class="btn btn-primary">Save</button></td>
            </tr>
          </form>
            <% }) %>
      </tbody>
    </table>
  </div>

